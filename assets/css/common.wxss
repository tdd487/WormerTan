/*none*/
.allLoaded {
  position: relative;
  text-align: center;
  font-size: 28rpx;
  color: #ccc;
  padding: 40rpx;
}
.allLoaded:before {
  position: absolute;
  left: 5%;
  top: 60rpx;
  width: 90%;
  z-index: 1;
  height: 2rpx;
  content: '';
  background: #ddd;
}
.line-word {
  position: relative;
  padding: 0 20rpx;
  z-index: 2;
  background: #e5e9f0;
}
